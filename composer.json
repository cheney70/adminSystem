{
    "name": "cheney/admin-system",
    "type": "library",
    "description": "Cheney Admin System - Laravel后台管理系统Composer包",
    "keywords": ["laravel", "admin", "rbac", "jwt"],
    "license": "MIT",
    "authors": [
        {
            "name": "Cheney Admin Team",
            "email": "support@cheney.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "laravel/framework": "^8.0|^9.0|^10.0",
        "tymon/jwt-auth": "^1.0",
        "darkaonline/l5-swagger": "^8.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.4.2",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Cheney\\AdminSystem\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cheney\\AdminSystem\\Tests\\": "src/tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cheney\\AdminSystem\\AdminSystemServiceProvider"
            ],
            "aliases": {
                "AdminAuth": "Cheney\\AdminSystem\\Facades\\AdminAuth",
                "AdminUser": "Cheney\\AdminSystem\\Facades\\AdminUser",
                "AdminRole": "Cheney\\AdminSystem\\Facades\\AdminRole",
                "AdminPermission": "Cheney\\AdminSystem\\Facades\\AdminPermission",
                "AdminMenu": "Cheney\\AdminSystem\\Facades\\AdminMenu",
                "AdminOperationLog": "Cheney\\AdminSystem\\Facades\\AdminOperationLog"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
